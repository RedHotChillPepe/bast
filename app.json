{
  "expo": {
    "name": "bast",
    "scheme": "myapp",
    "slug": "bast",
    "version": "1.0.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/BastLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/BastSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0077FF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bastithouses.app",
      "associatedDomains": [
        "applinks:win-e5oqtj6uhak.tailb0dc72.ts.net",
        "applinks:win-e5oqtj6uhak.tailb0dc72.ts.net"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/BastLogo.png",
        "backgroundColor": "#ffffff",
        "intentFilters": [
          {
            "action": "VIEW",
            "data": [
              {
                "scheme": "https",
                "host": "https://win-e5oqtj6uhak.tailb0dc72.ts.net/",
                "pathPrefix": "/share/*"
              },
              {
              "scheme": "myapp",
              "host": "*"
            }
            ],
            "category": [
              "BROWSABLE",
              "DEFAULT"
            ]
          }
        ]
      },
      "package": "com.bastithouses.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
